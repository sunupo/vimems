<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="headicon_style">
        <item name="android:layout_width">@dimen/dimen_80</item>
        <item name="android:layout_height">@dimen/dimen_80</item>
        <item name="android:layout_marginLeft">@dimen/dimen_5</item>
        <item name="android:layout_marginRight">@dimen/dimen_10</item>
        <item name="android:src">@drawable/ic_launcher_foreground</item>
    </style>

</resources>
