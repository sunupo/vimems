                android:layout_height="320px"
                android:gravity="left"
                android:layout_rowWeight="1"
                android:textSize="8sp"></TextView>
            <TextView
                android:text="four"
                android:id="@+id/muscle4_para_textview"
                android:layout_width="400px"
                android:layout_height="320px"
                android:gravity="left"
                android:layout_rowWeight="1"
                android:textSize="8sp"></TextView>
            <TextView
                android:text="five"
